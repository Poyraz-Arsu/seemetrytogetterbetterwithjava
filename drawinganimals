                System.out.println("    \\         | \\0|            |0/ |         /");
                System.out.println("    |          \\  |   .-==-.   |  /          |");
                System.out.println("     \\          `/`;          ;`\\`          /");
                System.out.println("      '.._      (_ |  .-==-.  | _)      _..'");
                System.out.println("          `\"`\"-`/ `/'        '\\` \\`-\"`\"`");
                System.out.println("               / /`;   .==.   ;`\\ \\");
                System.out.println("         .---./_/   \\  .==.  /   \\ \\");
                System.out.println("        / '.    `-.__)       |    `\"");
                System.out.println("       | =(`-.        '==.   ;");
                System.out.println("        \\  '. `-.           /");
                System.out.println("         \\_:_)   `\"--.....-'");
    
    
            break;    } 
            

            case("3"):{
           
                System.out.println("            ___   .--. ");
