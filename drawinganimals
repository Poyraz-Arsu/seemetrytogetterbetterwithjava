import java.util.Scanner;

public class DrawingAnimals {
public static void main(String[] args) {
    
    Scanner input = new Scanner(System.in);
    
    greetingsFunction();
    
    String number = input.nextLine();

    printingAnimals(number);
  //STILL TRYING TO USE FUNCTIONS TO BEST OF MY ABILITES  
    



}
     public static void greetingsFunction(){
                System.out.println("Welcome to my app,what animal would you like to draw?");
                System.out.println("Press 1 for Butterfly");
                System.out.println("Press 2 for Elephant");
                System.out.println("Press 3 for Bear");
                System.out.println("Press 4 for Snake");

            }
        








    public static void printingAnimals(String number){


        switch (number) {
            case ("1"):{
                System.out.println("  .==-.                   .-==.     ");
                System.out.println("   \\()8`-._  `.   .'  _.-'8()/     ");
