                System.out.println("    __/   \\`-.____.-' `\\      /");
                System.out.println("    | `---`'-'._/-`     \\----'    _");
                System.out.println("    |,-'`  /             |    _.-' `\\");
                System.out.println("   .'     /              |--'`     / |");
                System.out.println("  /      /\\              `         | |");
                System.out.println("  |   .\\/  \\      .--. __          \\ |");
                System.out.println("   '-'      '._       /  `\\         /");
                System.out.println("               `\\    '     |------'`");
                System.out.println("                 \\  |      |");
                System.out.println("                  \\        /");
                System.out.println("                   '._  _.'");
