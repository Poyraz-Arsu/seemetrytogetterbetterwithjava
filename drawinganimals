                System.out.println("      ,':8(o)./|=|\\.(o)8:`.        ");
                System.out.println("     (O :8 ::/ \\_/ \\:: 8: O)      ");
                System.out.println("      \\O `::/       \\::' O/       ");
                System.out.println("       \"\"--'         `--\"\"      ");
            break;    }
                
            case("2"):{
